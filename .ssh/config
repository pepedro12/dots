  Port 22
  IdentityFile ~/.ssh/id_ed25519
  # ServerAliveInterval 30
  # ServerAliveCountMax 4

Host company
  Port 22
  IdentityFile ~/.ssh/id_ed25519

Host server
  Port 22
  IdentityFile ~/.ssh/id_ed25519
  
Host github.com
    HostName ssh.github.com
    User git
    Port 443
    IdentityFile ~/.ssh/id_ed25519
    PreferredAuthentications publickey
