# SSH Config Example Template
# Copy this file to config and fill in your actual values
# Keep the real config file and sensitive data in config.local (not tracked by git)

Host <YOUR_HOST_ALIAS>
  HostName <YOUR_IP_OR_DOMAIN>
  User <YOUR_USERNAME>
  Port 22
  IdentityFile ~/.ssh/id_ed25519
  # ServerAliveInterval 30
  # ServerAliveCountMax 4

Host github.com
    HostName ssh.github.com
    User git
    Port 443
    IdentityFile ~/.ssh/id_ed25519
    PreferredAuthentications publickey
